{
	"name": "data_structures",
	"version": "1.0.0",
	"description": "Data Structures in JS",
	"keywords": ["DS", "Queue", "Priority", "Queue", "BST"],
	"author": "Jaspreet Singh",
	"dependencies": {
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@types/node": "^20.3.2",
		"@vitest/coverage-v8": "^0.32.2",
		"@vitest/ui": "^0.32.2",
		"dotenv": "^16.3.1",
		"esbuild": "^0.18.10",
		"husky": "^8.0.3",
		"pino": "^8.14.1",
		"pino-pretty": "^10.0.0",
		"rome": "^12.1.3",
		"tsx": "^3.12.7",
		"typedoc": "^0.24.8",
		"typescript": "^5.1.3",
		"vitest": "^0.32.2"
	},
	"license": "MIT",
	"type": "module",
	"private": true,
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"husky-setup": "husky install && chmod ug+x .husky/*",
		"lint": "rome check . --apply",
		"format": "rome format . --write",
		"fix": "pnpm format && pnpm lint",
		"check": "rome ci .",
		"typecheck": "tsc --noEmit -p tsconfig.build.json",
		"build": "node ci/esbuild.js",
		"test": "vitest run --no-threads",
		"test:coverage": "vitest run --no-threads --coverage",
		"test:ui": "vitest --no-threads --coverage --ui",
		"dev": "tsx dev/index.ts"
	},
	"volta": {
		"node": "18.16.0",
		"pnpm": "8.4.0"
	},
	"engines": {
		"node": ">=18",
		"pnpm": ">=8"
	},
	"packageManager": "pnpm@8.4.0"
}
